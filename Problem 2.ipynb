    "def sum_even_fibonacci(range):\n",
    "    a, b = 1, 2  \n",
    "    even_sum = 0  \n",
    "    while a < range:\n",
    "        if a % 2 == 0:  \n",
    "            even_sum += a  \n",
    "        a, b = b, a + b  \n",
    "    return even_sum\n",
